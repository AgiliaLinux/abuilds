pkgname=build-essential
pkgver=1.1
pkgbuild=1
arch=('noarch')

shortdesc="Generic build environment (meta package)"
tags="virtual dev-libs"

adddep="binutils gcc gcc-g++ make autoconf automake intltool kernel-headers gettext-tools pkg-config diffutils libxslt mkpkg"
build_deps="mkpkg"

build() {
	echo "Building virtual package $pkgname"
}

