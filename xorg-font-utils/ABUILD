pkgname=xorg-font-utils
pkgver=7.6
pkgbuild=1
arch=('noarch')

shortdesc="Vitual package for xorg font utilites"
source=("http://downloads.sourceforge.net/sourceforge/${pkgname}/${pkgname}-${pkgver}.tar.bz2")
tags="virtual x11-fonts"

#dependencies only needed to build package
build_deps="bdftopcf mkfontdir mkfontscale font-util"
adddep="bdftopcf mkfontdir mkfontscale font-util"


build() {
    echo "Building virtual package"
}

